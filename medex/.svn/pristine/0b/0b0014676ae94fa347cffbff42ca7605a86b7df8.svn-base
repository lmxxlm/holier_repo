.mainBody{ margin:12px; color:#555;}
.mainBody .col-md-3,.mainBody .col-lg-3,.mainBody .col-md-9,.mainBody .col-lg-9{ padding:0;}
.panel{ border:0; border-radius:0; box-shadow:none; margin-bottom:12px;}
.panel-heading{ padding:0; padding-left:12px; border-color:#EFEFEF !important;}
.panel-title{ height:35px; line-height:35px; color:#555;}
.panel-body{ padding:12px; padding-top:5px;}
.calendarList{ background:#fff; margin-right:12px; padding:0 12px 0 12px;}
.calendartitle{ height:35px; line-height:35px; color:#555; border-bottom:1px solid #EFEFEF; background:#fff; margin-right:12px; font-weight:bold; padding-left:12px; position:relative; font-size:16px;}
.calendartitle .glyphicon-time{ position:absolute; right:12px; top:10px; color:#ccc;}
.timeline-activity{ position:relative;}
.timeline-activity li{ padding:20px 0px 10px 0; border-left:1px solid #EFEFEF; margin-left:23px; position:relative;}
.timeline-activity li i.list-icon{ position:absolute;height:36px;width:36px;top:15px;left:-18px;font-size:18px;line-height:31px;border-radius:30px;text-align:center;background-color:#FFF;border:2px solid #DFDFDF;color:#DFDFDF;}
.timeline-activity li .block{ position:relative;padding:0px 20px 0px 36px;}
.timeline-activity li .caret{ position:absolute;width:8px;height:0px;top:10px;left:28px;border-width:8px 8px 8px 0px;border-style:solid solid solid dotted;border-color:transparent #DFDFDF transparent;}
.box-generic{border:1px solid #EFEFEF;position:relative;background:none repeat scroll 0% 0% #FFF;border-radius:5px;}
.timeline-top-info{ padding:10px; line-height:20px; border-bottom:1px solid #efefef;}
.timeline-bottom-info{ padding:7px 10px; font-size:12px;}
.comming.box-generic{ border:1px solid #F3CFFB; background-color:#FFF0FF;text-shadow:1px 1px 1px #fff; background-image:url(../../image/medex/upcoming.png); background-repeat:no-repeat; background-position:right bottom;}
.comming .timeline-top-info{ border-bottom-color:#DBB7E3;}
.comming .timeline-bottom-info .glyphicon{ color:#AF86B9;}
.comming .timeline-bottom-info a .glyphicon{ color:#337AB7;}
.box-generic.done{ background-image:url(../../image/medex/done.png); background-repeat:no-repeat; background-position:right bottom;}
.timeline-activity li:last-child .separator{ padding:0px 0px 50px;}
.timeline-activity li:hover i.list-icon{border-color:#BA96C2;color:#BA96C2;}
.addSchedule,.addSchedule:focus{ background-color:#BA96C2; border-color:#AF86B9; border-radius:0;text-shadow:0 1px 0 #333;}
.addSchedule:hover{ background-color:#AF86B9; border-color:#BA96C2;}
.timeAlert{ background:#fff; margin-top:12px; color:#424242;}
.timeAlert .panel-body{ background:#F7F9FA; border-bottom:1px solid #EFEFEF; padding:0; padding:30px 0;}
.timeAlert .panel-title{ height:30px; line-height:30px; font-size:12px; padding:0 12px; overflow:hidden;}
.timeAlert .line1{ font-size:12px; font-weight:bold; text-align:center;}
.timeAlert .line2{ font-size:30px; text-align:center; letter-spacing:2px; padding-top:5px;}
.modal-content{ padding:5px;}
.tabMenu{ background:#F7F9FA; padding:0; border-bottom:1px solid #efefef;}
.tabMenu li{ float:left; list-style:none; border-right:1px solid #EFEFEF;}
.tabMenu li a{ width:100%; height:100%; display:block; color:#999; font-weight:bold; padding:10px 12px; border-bottom:1px solid #F7F9FA; position:relative;}
.tabMenu li a:hover{ text-decoration:none;}
.tabMenu li.active a{ color:#555; background-color:#fff; border-bottom:1px solid #fff; top:1px;}
.tabMenu li i{ font-size:16px; padding-top:4px;}
.addScheduleCon{ padding:0px 38px 15px 38px;}
.addScheduleStep1 .form-group{ padding:10px 38px; background:#BA96C2; color:#fff;}
.form-repeat{ display:none;}
.notebook { min-height:60px; line-height:30px; background:-webkit-linear-gradient(bottom, #f0f0f0 0%, #ffffff 5%) 0 0;background:-moz-linear-gradient(bottom, #f0f0f0 0%, #ffffff 5%) 0 0;background:-ms-linear-gradient(bottom, #f0f0f0 0%, #ffffff 5%) 0 0;-webkit-background-size: 100% 30px;-moz-background-size: 100% 30px;-ms-background-size: 100% 30px;background-size: 100% 30px; padding:0 10px;}
#cure{ display:none;}
.timeSolution .td2 .form-control{ display:none;}
.smartSearch{ padding:0; position:relative;}
.smartSearch ul{ position:absolute; top:100%; left:0; background:#fff; border:1px solid #CCC; border-top:none; padding:0; display:none; box-shadow: 0px 6px 12px rgba(0, 0, 0, 0.176);z-index:9999}
.smartSearch li{ list-style:none;}
.smartSearch li:nth-child(even){ background:#F9F9F9;}
.smartSearch li a{ padding:5px 12px; margin:0; display:block; color:#505050; min-width:300px;}
.smartSearch li a:hover{ background:#ccc; text-decoration:none; color:#fff;}